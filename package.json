{
    "name": "identification-rumble",
    "version": "0.0.1",
    "description": "Identification Rumble",
    "repository": "https://github.com/marc1404/identification-rumble.git",
    "author": "Marc Vornetran <marc.vornetran@gmail.com>",
    "license": "MIT",
    "private": true,
    "scripts": {
        "precommit": "pretty-quick --staged",
        "dev:frontend": "nuxt",
        "dev:api": "nodemon api/index.ts",
        "start:api": "forever -c ts-node api/index.ts",
        "generate": "yarn add mime-db && nuxt generate"
    },
    "dependencies": {
        "@nuxtjs/google-analytics": "2.0.2",
        "@types/dotenv": "4.0.2",
        "@types/jsonfile": "4.0.1",
        "@types/node": "9.4.0",
        "@types/socket.io": "1.4.31",
        "bootstrap": "4.0.0",
        "chartist": "0.11.0",
        "date-fns": "1.29.0",
        "flag-icon-css": "2.9.0",
        "forever": "0.15.3",
        "jsonfile": "4.0.0",
        "keypress.js": "2.1.4",
        "lodash.debounce": "4.0.8",
        "lodash.throttle": "4.1.1",
        "noty": "3.2.0-beta",
        "nuxt": "1.2.1",
        "oridomi": "1.1.2",
        "qrcode": "1.2.0",
        "socket.io": "2.0.4",
        "socket.io-client": "2.0.4",
        "ts-node": "4.1.0",
        "typescript": "2.6.2"
    },
    "devDependencies": {
        "dotenv": "4.0.0",
        "husky": "0.14.3",
        "nodemon": "1.14.11",
        "prettier": "1.10.2",
        "pretty-quick": "1.2.3"
    },
    "nodemonConfig": {
        "ext": "*.ts",
        "exec": "ts-node",
        "watch": "api"
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": true
    }
}
